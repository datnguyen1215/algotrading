    "expressjs": "^1.0.1",
    "nodemon": "^3.0.3"
  },
  "dependencies": {
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "module-alias": "^2.2.3",
    "pg": "^8.11.3",
    "pg-format": "^1.0.4"
  }
